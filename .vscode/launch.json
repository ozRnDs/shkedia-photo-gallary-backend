{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: Main",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": "src/manage.py",
            // "module": "uvicorn",
            "args": ["runserver","0.0.0.0:8000"],
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "JWT_KEY_LOCATION": "/temp/jwt_token",
                "TOKEN_TIME_PERIOD": "1",
                "PUBLIC_KEY_LOCATION": "/temp/data.pub",
                "PRIVATE_KEY_LOCATION": "/temp/data",
                "LOCAL_CACHE_LOCATION": "${workspaceFolder}/.local/albums_list.pickle",
            }
        }
    ]
}