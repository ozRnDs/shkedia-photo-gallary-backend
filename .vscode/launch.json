{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: Main",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": "src/manage.py",
            // "module": "uvicorn",
            "args": ["runserver","0.0.0.0:8000"],
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "JWT_KEY_LOCATION": "/temp/jwt_token",
                "TOKEN_TIME_PERIOD": "120",
                "PUBLIC_KEY_LOCATION": "/temp/data.pub",
                "PRIVATE_KEY_LOCATION": "/temp/data",
                "LOCAL_CACHE_LOCATION": "${workspaceFolder}/.local/albums_list.pickle",
                "MEDIA_DB_HOST": "10.0.0.5",
                "MEDIA_DB_PORT": "4431",
                "MEDIA_REPO_HOST": "10.0.0.5",
                "MEDIA_REPO_PORT": "4432",
                "USER_DB_HOST": "10.0.0.5",
                "USER_DB_PORT": "4430",
                "THUMBNAIL_MAX_WIDTH": "500",
                "THUMBNAIL_MAX_HEIGHT": "500",
                "DEBUG": "1",
                "ALLOWED_HOST": "10.0.0.5",
                "UPLOAD_URL": "http://10.0.0.5:25001/images/v2"
            }
        }
    ]
}